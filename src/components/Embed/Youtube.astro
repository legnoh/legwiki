---
type Props = {
	id: string;
};
const { id } = Astro.props as Props;
const youtubeUrl = `https://www.youtube.com/embed/${id}`
---
<iframe src={youtubeUrl} title="YouTube video player" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe>
