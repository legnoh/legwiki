---
type Props = {
  url: string;
};
const { url } = Astro.props as Props;
---

<blockquote
  class="instagram-media"
  data-instgrm-captioned
  data-instgrm-permalink={url}
  data-instgrm-version="12"
>
  <a href={url} target="_blank">Instagram</a>
</blockquote>
<script async is:inline src="//www.instagram.com/embed.js"></script>
